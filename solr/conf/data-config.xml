<!--
  Data Import Configuration

-->
<dataConfig>

	<dataSource 
		type="JdbcDataSource" 
		driver="com.mysql.jdbc.Driver" 
		url="jdbc:mysql://localhost/trac12" 
		user="tracuser"
		password="password"
		batchSize="1000"
	/>

	<document>
		<entity 
			name="wiki" 
			query="select name, version, from_unixtime(time/1000000) as time, author, text, comment from wiki">

		</entity>
	</document>

</dataConfig>
